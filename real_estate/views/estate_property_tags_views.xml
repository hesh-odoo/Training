<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="estate_property_tags_views" model="ir.ui.view">
            <field name="name">estate.property.tags.views</field>
            <field name="model">estate.property.tags</field>
            <field name="arch" type="xml">
                <tree string="Estate" editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="estate_property_tags_view_form" model="ir.ui.view">
            <field name="name">estate.property.tags.form</field>
            <field name="model">estate.property.tags</field>
            <field name="arch" type="xml">
                <form string="Estate Property">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_estate_property_tags">
            <field name="name">Property tags</field>
            <field name="res_model">estate.property.tags</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>